{
  "name": "@sero.run/sero",
  "version": "0.0.9",
  "description": "Sero is a modular TypeScript toolchain for FHIR (and friends) sponsored by Automate Medical Inc.",
  "homepage": "https://sero.run",
  "bugs": "https://github.com/automate-medical/sero/issues",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "eslint . && tsc -p tsconfig.json",
    "start": "ts-node example/index.ts -p tsconfig.json",
    "test": "jest --detectOpenHandles",
    "docs": "typedoc src/index.ts src/cds-hooks/index.ts src/http/index.ts src/client/index.ts src/rest/index.ts --readme ./README.md",
    "lint": "eslint . --fix",
    "examples": "npm run build && npm run start"
  },
  "keywords": [
    "fhir",
    "hl7",
    "cds-hooks"
  ],
  "author": "Automate Medical Inc.",
  "license": "Apache 2.0",
  "exports": {
    "./cds-hooks": "./dist/cds-hooks/index.js",
    "./http": "./dist/http/index.js",
    "./rest": "./dist/rest/index.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "cross-fetch": "^3.1.4",
    "fastify": "^3.18.0"
  },
  "devDependencies": {
    "@types/fhir": "^0.0.34",
    "@types/jest": "^26.0.23",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "pino-pretty": "^5.1.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=14.17"
  }
}
