{
  "compilerOptions": {
    "lib": ["ES6"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": false,
    "removeComments": true,
		"skipLibCheck": true
  },
	"include": ["src/**/*"],
  "exclude": ["src/**/*.test.ts"]
}